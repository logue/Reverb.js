{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@logue/reverb",
  "version": "1.2.18",
  "description": "JavaScript Reverb effect class",
  "keywords": [
    "webaudio",
    "convolver",
    "effect",
    "reverb"
  ],
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Logue",
    "email": "logue@hotmail.co.jp",
    "url": "https://logue.dev/"
  },
  "homepage": "https://github.com/logue/Reverb.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/Reverb.js.git"
  },
  "bugs": {
    "url": "https://github.com/logue/Reverb.js/issues"
  },
  "files": [
    "dist"
  ],
  "main": "dist/Reverb.umd.js",
  "module": "dist/Reverb.es.js",
  "types": "dist/src/Reverb.d.ts",
  "exports": {
    ".": {
      "import": "./dist/Reverb.es.js",
      "require": "./dist/Reverb.umd.js",
      "types": "./dist/src/Reverb.d.ts"
    }
  },
  "engines": {
    "node": ">=18.16.0",
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@3.6.1",
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "clean": "rimraf node_modules/.vite",
    "type-check": "tsc --noEmit --composite false",
    "build": "run-p type-check build-only",
    "build:docs": "vite build --mode=docs",
    "build:analyze": "vite build --mode=analyze",
    "build:clean": "rimraf dist",
    "build-only": "vite build",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . --write",
    "preview": "vite preview --mode=docs",
    "prepare": "husky install"
  },
  "dependencies": {
    "@thi.ng/colored-noise": "^1.0.19",
    "@thi.ng/transducers": "^8.5.0"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^18.12.3",
    "@types/node": "^20.4.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "bootstrap": "^5.3.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "latest",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-yaml": "^0.5.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "typescript": "latest",
    "vite": "^4.4.9",
    "vite-plugin-banner": "^0.7.0",
    "vite-plugin-checker": "^0.6.1",
    "vite-plugin-dts": "^3.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,htm,html}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "http-cache-semantics": "^4.1.1",
    "json5": "^2.2.3",
    "semver": "^7.5.2",
    "yaml": "^2.2.2"
  }
}
