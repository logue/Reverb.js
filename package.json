{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@logue/reverb",
  "version": "1.4.1",
  "description": "JavaScript Reverb effect class",
  "keywords": [
    "webaudio",
    "convolver",
    "effect",
    "reverb"
  ],
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Logue",
    "email": "logue@hotmail.co.jp",
    "url": "https://logue.dev/"
  },
  "homepage": "https://github.com/logue/Reverb.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/Reverb.js.git"
  },
  "bugs": {
    "url": "https://github.com/logue/Reverb.js/issues"
  },
  "files": [
    "dist"
  ],
  "main": "dist/Reverb.umd.js",
  "module": "dist/Reverb.es.js",
  "browser": "dist/Reverb.iife.js",
  "types": "dist/Reverb.d.ts",
  "exports": {
    ".": {
      "import": "./dist/Reverb.es.js",
      "types": "./dist/Reverb.d.ts"
    }
  },
  "engines": {
    "node": ">=22.0.1",
    "pnpm": ">=10.3.0"
  },
  "packageManager": "pnpm@10.12.1",
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "clean": "rimraf node_modules/.vite",
    "type-check": "tsc --noEmit --composite false",
    "build": "run-p type-check build-only",
    "build:docs": "vite build --mode=docs",
    "build:analyze": "vite build --mode=analyze",
    "build:clean": "rimraf dist",
    "build-only": "vite build",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . --write",
    "preview": "vite preview --mode=docs",
    "prepare": "husky"
  },
  "dependencies": {
    "@thi.ng/colored-noise": "^1.0.102",
    "@thi.ng/random": "^4.1.20",
    "@thi.ng/transducers": "^9.4.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@tsconfig/node-lts": "^22.0.2",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "bootstrap": "^5.3.7",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-custom-alias": "^1.3.2",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-yaml": "^1.1.2",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^16.1.2",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^6.3.5",
    "vite-plugin-banner": "^0.8.1",
    "vite-plugin-checker": "^0.9.3",
    "vite-plugin-dts": "^4.5.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,htm,html}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "json5": ">=2.2.3",
    "yaml": ">=2.3.2"
  }
}
